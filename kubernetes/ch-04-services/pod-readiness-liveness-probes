apiVersion: v1
kind: Pod
metadata:
  name: probeone
  labels:
    app: apache
spec:
  containers:
  - name: boxone
    image: lovelearnlinux/webserver:v1
    ports:
    - containerPort: 80
    readinessProbe:
      tcpSocket:
        port: 80
      initialDelaySeconds: 5
      periodSeconds: 10
    livenessProbe:
      tcpSocket:
        port: 80
      initialDelaySeconds: 15
      periodSeconds: 20
